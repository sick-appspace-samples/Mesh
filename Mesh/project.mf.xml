<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<manifest>
    <application name="Mesh">
        <crown name="Mesh">
            <desc>user script</desc>
        </crown>
        <meta key="author">AppSpace Sample</meta>
        <meta key="version">2.5.0</meta>
        <meta key="licensed">false</meta>
        <meta key="priority">low</meta>
        <meta key="copy-protected">false</meta>
        <meta key="read-protected">false</meta>
        <entry path="pages" default="PointCloudViewer.msdd">
            <file type="application/zip">PointCloudViewer.msdd</file>
        </entry>
        <entry path="resources">
            <file type="application">Box.ply</file>
            <file type="application">Hinge.ply</file>
        </entry>
        <entry path="scripts" default="transform.lua">
            <file type="text/x-lua">cad.lua</file>
            <file type="text/x-lua">create.lua</file>
            <file type="text/x-lua">findConnected.lua</file>
            <file type="text/x-lua">pointcloud.lua</file>
            <file type="text/x-lua">project.lua</file>
            <file type="text/x-lua">shape.lua</file>
            <file type="text/x-lua">transform.lua</file>
        </entry>
    </application>
</manifest>
